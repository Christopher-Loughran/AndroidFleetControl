{
    "name": "fleet-control",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "concurrently \"npm --prefix server run test\" \"ng serve\"",
        "prod": "concurrently \"npm --prefix server run prod\" \"ng serve --host 0.0.0.0 --disable-host-check --port 4200\"",
        "init": "npm i && npm --prefix server i",
        "serve": "concurrently \"ng serve --host 0.0.0.0 --disable-host-check\" \"nodemon server/server.js\""
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.1.0",
        "@angular/common": "~13.1.0",
        "@angular/compiler": "~13.1.0",
        "@angular/core": "~13.1.0",
        "@angular/forms": "~13.1.0",
        "@angular/platform-browser": "~13.1.0",
        "@angular/platform-browser-dynamic": "~13.1.0",
        "@angular/router": "~13.1.0",
        "@fortawesome/angular-fontawesome": "^0.10.1",
        "@fortawesome/fontawesome-svg-core": "~1.2.36",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "angular-file-uploader": "^7.1.0",
        "bootstrap": "^5.1.3",
        "concurrently": "^6.3.0",
        "connect-busboy": "0.0.2",
        "file-saver": "^2.0.5",
        "jquery": "^3.6.0",
        "material-icons": "^1.10.4",
        "rxjs": "~6.6.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.1.1",
        "@angular/cli": "~13.1.1",
        "@angular/compiler-cli": "~13.1.0",
        "@types/jasmine": "~3.8.0",
        "@types/node": "^12.11.1",
        "jasmine-core": "~3.8.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "nodemon": "^2.0.14",
        "typescript": "~4.5.3"
    }
}